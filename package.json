{
  "name": "terraform-doc-snippets",
  "displayName": "terraform-doc-snippets",
  "description": "Get terraform code snippets straight from the provider docs.",
  "version": "0.0.1",
  "publisher": "Run at Scale",
  "engines": {
    "vscode": "^1.19.0"
  },
  "author": {
    "name": "Brandon O'Connor",
    "email": "brandon@atscale.run"
  },
  "categories": ["Snippets"],
  "repository": {
    "type": "git",
    "url": "https://github.com/run-at-scale/terraform-doc-snippets.git"
  },
  "contributes": {
    "snippets": [
      {
        "language": "terraform",
        "path": "./snippets/terraform.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./src",
    "watch": "tsc -watch -p ./src",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "postversion": "npm run publish:release",
    "publish:vsce": "vsce publish",
    "publish:release": "conventional-github-releaser -p angular -r 0",
    "build:snippets": "ts-node ./src/main.ts",
    "build:changelog":
      "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "@types/fs-extra": "^5.0.2",
    "@types/lockfile": "^1.0.0",
    "@types/tmp": "0.0.33",
    "@types/underscore": "^1.8.8",
    "async": "^2.6.0",
    "conventional-changelog": "^1.1.7",
    "conventional-github-releaser": "^2.0.0",
    "eslint": "^4.19.1",
    "gfm-code-blocks": "^1.0.0",
    "lockfile": "^1.0.4",
    "markdown": "^0.5.0",
    "nodegit": "^0.20.3",
    "tmp": "0.0.33",
    "tslint": "^5.9.1",
    "underscore": "^1.9.0"
  }
}
